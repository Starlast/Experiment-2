#include <iostream>

using namespace std;

int main()
{ 
    int n1 = 0;
    int n2 = 1;
    int n3;
    
    cout<<"  The Fibonacci numbers";
    cout <<""<< endl;
    cout <<""<< endl;
    cout<<n1<<","<<n2<<",";
    for(int i = 3; i<=22; i++)
    {
        n3 = n1 + n2;
        n1 = n2;
        n2 = n3;
        cout<<n3;
        if(i <22)
        {
            cout<<",";
            
        }
    }
    
    return 0;
}
