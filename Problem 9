#include <iostream>

using namespace std;

int main()
{
    int rows, column;
    
    cout<<"  Enter the row:     "; cin>> rows;
    cout<<"  Enter the column:  "; cin>> column;
    cout <<""<< endl;
    cout <<""<< endl;
    for(int i=1; i<=rows; i++){
        //cout<<"* "<<endl;
        for(int j=1; j<=column; j++){
            cout<<" *";
            
            
        }
        cout<<endl;
        
        
    }

}
